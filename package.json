{
  "name": "flowbitai",
  "version": "1.0.0",
  "type": "module",
  "description": "FlowbitAI Backend API",
  "main": "dist/api/index.js",
  "scripts": {
    "seed": "tsx seed.ts",
    "dev": "tsx api/index.ts",
    "dev:watch": "tsx watch api/index.ts",
    "start": "tsx api/index.ts",
    "build": "tsc",
    "start:build": "node dist/api/index.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:api": "eslint api/",
    "lint:react-shell": "eslint react-shell/src/",
    "lint:support-app": "eslint support-app/src/",
    "test": "jest",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run --spec 'cypress/e2e/**/*.cy.{js,jsx,ts,tsx}'",
    "test:e2e:headed": "cypress run --headed --spec 'cypress/e2e/**/*.cy.{js,jsx,ts,tsx}'",
    "test:smoke": "cypress run --spec 'cypress/e2e/smoke-test.cy.ts'",
    "test:all": "npm run test && npm run test:e2e"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.19",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.11.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.5",
    "@eslint/js": "^9.31.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "cypress": "^14.5.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^30.0.4",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
